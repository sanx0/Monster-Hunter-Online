        <QuestTalk QuestID="1602">
            <ExceTalk>
                <Rounds>
                    <Round RoundID="1">
                        <Text>请问有什么可以帮你 的吗？</Text>
                        <GotoRound Title="秋梨欧想要知道关于猎人武器的一些事情" RoundID="2" />
                    </Round>
                    <Round RoundID="2">
